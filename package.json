{
  "name": "agentary-js",
  "version": "1.4.6",
  "description": "JS SDK for running quantized small language models in the browser",
  "type": "module",
  "private": false,
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "pack:test": "npm pack && npm install -g ./agentary-js-*.tgz",
    "pack:clean": "rm -f agentary-js-*.tgz",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:start": "cd docs && npm run start"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.0.0",
    "js-tiktoken": "^1.0.21"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "openai": "^6.7.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^24.8.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "jsdom": "^27.0.0",
    "next": "^15.5.6",
    "nextra": "^3.3.1",
    "nextra-theme-docs": "^3.3.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.1.0",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://agentary-js.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentary-ai/agentary-js.git"
  },
  "bugs": {
    "url": "https://github.com/agentary-ai/agentary-js/issues"
  },
  "keywords": [
    "ai",
    "llm",
    "webgpu",
    "webassembly",
    "browser",
    "quantized",
    "inference",
    "agent",
    "workflow",
    "transformers",
    "javascript",
    "typescript",
    "client-side",
    "onnx"
  ],
  "author": "Declan Healey <declan@agentary.ai>",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
